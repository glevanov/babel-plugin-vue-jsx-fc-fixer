{
    "extends": ["eslint:recommended", "plugin:jest/recommended"],
    "ignorePatterns": ["dist/*"],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2020
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "jest/valid-title": "off",
        "indent": ["error", "tab"]
    }
}